{
  "name": "battis/phpunit-sessions",
  "description": "Extension to PHPUnit to handle PHP sessions more gracefully",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Seth Battis",
      "email": "seth@battis.net"
    }
  ],
  "require": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Battis\\PHPUnit\\Sessions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Battis\\PHPUnit\\Sessions\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
