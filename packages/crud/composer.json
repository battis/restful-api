{
  "name": "battis/crud",
  "description": "Doctrine DBAL-based CRUD implementation",
  "authors": [
    {
      "name": "Seth Battis",
      "email": "seth@battis.net"
    }
  ],
  "require": {
    "ext-pdo": "*",
    "battis/data-utilities": "^1.1",
    "battis/hydratable": "^0.1",
    "envms/fluentpdo": "^2.2"
  },
  "require-dev": {
    "battis/phpunit-pdo": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Battis\\CRUD\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Battis\\CRUD\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
